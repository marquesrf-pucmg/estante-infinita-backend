model Usuario {
  id           Int          @id @default(autoincrement())
  nome         String
  email        String       @unique
  senha        String
  criadoEm     DateTime     @default(now())
  atualizadoEm DateTime     @updatedAt
  anuncios     Anuncio[]
  avaliacoes   Avaliacao[]
  comentarios  Comentario[]
}
